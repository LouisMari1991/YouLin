<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="SwipeMenuLayout">
    <attr format="boolean" name="swipeEnable" />
    <attr format="boolean" name="ios" />
    <attr format="boolean" name="leftSwipe" />
  </declare-styleable>

  <!--
    <attr format="dimension" name="indexBarTextSize" />
  -->

  <declare-styleable name="IndexBar">
    <attr format="dimension" name="indexBarTextSize" />
    <attr format="color" name="indexItemPressColor" />
    <attr format="color" name="indexItemNormalColor" />
  </declare-styleable>

  <declare-styleable name="CheckableView">
    <attr format="reference" name="normalDrawable" />
    <attr format="reference" name="checkedDrawable" />
  </declare-styleable>

  <!-->Chat input style<-->
  <attr name="chat_input" />
  <declare-styleable name="ChatInputView">
    <attr format="dimension|reference" name="inputMarginLeft" />
    <attr format="dimension|reference" name="inputMarginRight" />
    <attr format="reference" name="inputEditTextBg" />
    <attr format="integer" name="inputMaxLines" />
    <attr format="string" name="inputHint" />
    <attr format="string" name="inputText" />
    <attr format="dimension|reference" name="inputTextSize" />
    <attr format="color|reference" name="inputTextColor" />
    <attr format="color|reference" name="inputHintColor" />
    <attr format="reference" name="inputCursorDrawable" />
    <attr format="reference" name="sendBtnBg" />
    <attr format="reference" name="sendBtnIcon" />
    <attr format="reference" name="sendCountBg" />
    <attr format="reference" name="voiceBtnBg" />
    <attr format="reference" name="voiceBtnIcon" />
    <attr format="reference" name="cameraBtnBg" />
    <attr format="reference" name="cameraBtnIcon" />
    <attr format="reference" name="photoBtnBg" />
    <attr format="reference" name="photoBtnIcon" />
  </declare-styleable>

  <declare-styleable name="RecordVoiceButton">
    <attr format="string" name="voiceBtnText" />
    <attr format="string" name="tapDownText" />
    <attr format="reference" name="recordVoiceBtnBg" />
    <attr format="reference" name="mic_1" />
    <attr format="reference" name="mic_2" />
    <attr format="reference" name="mic_3" />
    <attr format="reference" name="mic_4" />
    <attr format="reference" name="mic_5" />
    <attr format="reference" name="cancelRecord" />
  </declare-styleable>

  <declare-styleable name="RecordVoiceDialog">
    <attr format="reference" name="background" />
  </declare-styleable>

  <declare-styleable name="ProgressButton">
    <attr format="color" name="roundColor" />
    <attr format="color" name="roundProgressColor" />
    <attr format="dimension" name="roundWidth" />
    <attr format="integer" name="max" />
  </declare-styleable>

  <!-->Message List<-->
  <attr name="message_list" />
  <declare-styleable name="MessageList">
    <attr format="dimension|reference" name="dateTextSize" />
    <attr format="color|reference" name="dateTextColor" />
    <attr format="string|reference" name="dateFormat" />
    <attr format="dimension|reference" name="datePadding" />
    <attr format="dimension|reference" name="eventTextSize" />
    <attr format="color|reference" name="eventTextColor" />
    <attr format="dimension|reference" name="eventPadding" />

    <attr format="dimension|reference" name="avatarWidth" />
    <attr format="dimension|reference" name="avatarHeight" />
    <attr format="integer" name="showDisplayName">
      <enum name="HIDE" value="0" />
      <enum name="SHOW" value="1" />
    </attr>

    <attr format="reference" name="receiveBubbleDrawable" />
    <attr format="color|reference" name="receiveBubbleColor" />
    <attr format="color|reference" name="receiveBubblePressedColor" />
    <attr format="color|reference" name="receiveBubbleSelectedColor" />
    <attr format="dimension|reference" name="receiveBubblePaddingLeft" />
    <attr format="dimension|reference" name="receiveBubblePaddingTop" />
    <attr format="dimension|reference" name="receiveBubblePaddingRight" />
    <attr format="dimension|reference" name="receiveBubblePaddingBottom" />
    <attr format="color|reference" name="receiveTextColor" />
    <attr format="dimension|reference" name="receiveTextSize" />

    <attr format="reference" name="sendBubbleDrawable" />
    <attr format="color|reference" name="sendBubbleColor" />
    <attr format="color|reference" name="sendBubblePressedColor" />
    <attr format="color|reference" name="sendBubbleSelectedColor" />
    <attr format="dimension|reference" name="sendBubblePaddingLeft" />
    <attr format="dimension|reference" name="sendBubblePaddingTop" />
    <attr format="dimension|reference" name="sendBubblePaddingRight" />
    <attr format="dimension|reference" name="sendBubblePaddingBottom" />
    <attr format="color|reference" name="sendTextColor" />
    <attr format="dimension|reference" name="sendTextSize" />
    <attr format="float|reference" name="bubbleMaxWidth" />
    <attr format="reference" name="sendingProgressDrawable" />
    <attr format="reference" name="sendingIndeterminateDrawable" />

    <attr format="reference" name="sendVoiceDrawable" />
    <attr format="reference" name="receiveVoiceDrawable" />
    <attr format="reference" name="playSendVoiceAnim" />
    <attr format="reference" name="playReceiveVoiceAnim" />

    <attr format="reference" name="sendPhotoMsgBg" />
    <attr format="reference" name="receivePhotoMsgBg" />

    <attr format="dimension|reference" name="videomessage_radius" />
  </declare-styleable>


   <declare-styleable name="SwitchButton">
     <attr name="kswThumbDrawable" format="reference"/>
     <attr name="kswThumbColor" format="color|reference"/>
     <attr name="kswThumbMargin" format="dimension|reference"/>
     <attr name="kswThumbMarginTop" format="dimension|reference"/>
     <attr name="kswThumbMarginBottom" format="dimension|reference"/>
     <attr name="kswThumbMarginLeft" format="dimension|reference"/>
     <attr name="kswThumbMarginRight" format="dimension|reference"/>
     <attr name="kswThumbWidth" format="dimension|reference"/>
     <attr name="kswThumbHeight" format="dimension|reference"/>
     <attr name="kswThumbRadius" format="dimension|reference"/>
     <attr name="kswBackRadius" format="dimension|reference"/>
     <attr name="kswBackDrawable" format="reference"/>
     <attr name="kswBackColor" format="color|reference"/>
     <attr name="kswFadeBack" format="boolean"/>
     <attr name="kswBackMeasureRatio" format="float"/>
     <attr name="kswAnimationDuration" format="integer"/>
     <attr name="kswTintColor" format="color|reference"/>
     <attr name="kswTextOn" format="string"/>
     <attr name="kswTextOff" format="string"/>
     <attr name="kswTextMarginH" format="dimension"/>
     <attr name="kswAutoAdjustTextPosition" format="boolean"/>
   </declare-styleable>

  <!--加载空界面-->
  <declare-styleable name="EmptyLayout">
    <attr format="color" name="background_color" />
    <attr format="reference" name="no_data_drawable" />
    <attr format="string" name="no_data_str" />
  </declare-styleable>

</resources>